{
  "name": "react-gsap",
  "version": "3.3.0",
  "description": "React components for GSAP",
  "author": "bitworking",
  "license": "MIT",
  "repository": "bitworking/react-gsap",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-gsap.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format cjs,esm,umd",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "typecheck": "tsc --project tsconfig.json -noEmit"
  },
  "peerDependencies": {
    "gsap": ">=3",
    "react": "^16.0.0-0 || ^17.0.0-0 || ^18.0.0-0",
    "react-dom": "^16.0.0-0 || ^17.0.0-0 || ^18.0.0-0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "endOfLine": "lf"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.19.1",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-is": "^17.0.3",
    "gsap": "^3.11.1",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "react-is": "^18.2.0"
  }
}
